{# BOTON PARA IMPRIMIR EL OBJETO #}
{% if values.permisos.permisosModulo['LI'] and (values.template == values.controller ~ '/edit.html.twig') %}
    <button class="btn green" onclick="$('#accion_{{values.controller}}').val('Imprimir');submitForm('manto_{{values.controller}}');">
        <i class="fa fa-print"></i>&nbsp;Imprimir
    </button>        
{% endif %}
